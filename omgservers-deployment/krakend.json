{
  "$schema": "https://www.krakend.io/schema/krakend.json",
  "version": 3,
  "name": "OMGSERVERS - KrakenD - API Gateway",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "endpoints": [
    {
      "endpoint": "/omgservers/admin-api/v1/request/ping-server",
      "method": "PUT",
      "input_headers": [
        "Host",
        "User-Agent",
        "Content-Type",
        "Authorization",
        "X-Forwarded-Proto",
        "X-Forwarded-For"
      ],
      "backend": [
        {
          "url_pattern": "/omgservers/admin-api/v1/request/ping-server",
          "method": "PUT",
          "host": [
            "http://omgservers-service-1:8080",
            "http://omgservers-service-2:8080"
          ]
        }
      ]
    }
  ]
}